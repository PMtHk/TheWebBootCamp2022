<% layout("/layouts/boilerplate") %>
<h1><%=targetCamp.title%></h1>
<h2><%=targetCamp.location%></h2>
<img src="<%=targetCamp.img%>" alt="image" />
<p><%= targetCamp.description%></p>
<footer>
  <a href="/campgrounds/<%=targetCamp._id%>/edit">Edit</a>
  <form action="/campgrounds/<%=targetCamp._id%>?_method=DELETE" method="POST">
    <button>Delete</button>
  </form>
  <a href="/campgrounds">All Campground</a>
</footer>
